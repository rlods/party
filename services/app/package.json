{
  "name": "party-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "start": "PORT=8080 CHOKIDAR_USEPOLLING=true react-scripts start",
    "serve": "serve -s build -p 8080",
    "test": "PORT=8080 react-scripts test --verbose --forceExit --runInBand --detectOpenHandles --coverage --no-cache",
    "test:watch": "PORT=8080 react-scripts test --verbose --forceExit --runInBand --detectOpenHandles --no-cache  --watchAll",
    "lint": "./node_modules/.bin/eslint --ext js,ts,tsx ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "cookies-js": "^1.2.3",
    "firebase": "^7.13.2",
    "i18next": "^19.3.4",
    "jimp": "^0.10.0",
    "jsonp": "^0.2.1",
    "moment": "^2.24.0",
    "qs": "^6.9.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
