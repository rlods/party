    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-transition-group": "^2.9.0",
    "@types/uuid": "^7.0.2",
    "node-sass": "^4.13.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.8.3"
  }
